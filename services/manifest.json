{
  "manifest_version": 3,
  "name": "CGN Fast Search",
  "description": "Search CheapGraphicNovels.com quickly",
  "version": "1.1",
  "action": {
    "default_title": "CGN Fast Search",
    "default_icon": {
      "16": "pow.png",
      "48": "pow.png",
      "128": "pow.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["scripting", "tabs"],
  "host_permissions": [
    "https://cheapgraphicnovels.com/*"
  ],
  "icons": {
    "16": "pow.png",
    "48": "pow.png",
    "128": "pow.png"
  }
}
